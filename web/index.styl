@import "conform"

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, section, summary, time, mark, audio, video
	margin 0
	padding 0
	border 0
	font inherit
	vertical-align baseline

body, html
	background-color #313131
	font-family 'Monaco', courier, monospace
	color #cccccc

body
	padding 0 1.5em 1.5em 1.5em
	font-size 120%

ul
	margin-left 1.5em

h2
	font-size 160%
	font-weight bold

	.value
		color #cb4b16

	a
		font-size 50%
		margin-left 2.0em

h1
	font-size 160%
	padding 0.5em
	font-weight bold

	a
		font-size 40%
		margin-left 2.0em

a, a:visited
	color #268bd2

p, li
	line-height 1.4
	font-size 120%

hr
	height 1px
	border 0
	background-linear-gradient left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0)

pre
	border 1px solid black

strong
	font-weight bold

.code
	background #002B36
	color #839496
	padding 0.3em 1.0em
	border 1px solid black

table
	color #839496
	border-spacing 0
	border-collapse collapse
	margin-bottom 4.0em

	th, td
		border 1px solid black

	th
		background-color #073642
		font-weight bold
		vertical-align bottom
		padding 0.5em 1.0em 0.2em 1.0em
		white-space nowrap

	td
		background-color #002b36
		text-align right
		white-space nowrap
		vertical-align top
		padding 0.2em 0.5em 1.0em 1.0em

		.size
			margin-left 0

	td:last-child
		text-align left
		white-space normal

table.setListTable
	td:nth-child(2)
		text-align center

.symbolsSection
	div.symbol
		display inline-block
		text-align center
		margin-right 0.5em
		width 3.0em
		margin-bottom 1.0em

		div
			display inline-block
			width 32px
			height 32px
			vertical-align middle
			line-height 32px

